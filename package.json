{
  "name": "vscode-git-remote",
  "preview": true,
  "displayName": "Git Remote File System",
  "description": "View your Git remote repo in VS Code without cloning",
  "version": "0.0.11",
  "publisher": "vespa-dev-works",
  "repository": {
    "url": "https://github.com/nithinpeter/vscode-git-remote.git",
    "type": "git"
  },
  "keywords": [
    "git",
    "github",
    "bitbucket",
    "gitlab",
    "git provider",
    "remote",
    "filesystem provider",
    "remote filesystem",
    "git filesystem"
  ],
  "icon": "assets/logo.png",
  "galleryBanner": {
    "theme": "dark",
    "color": "#545454"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "GPFS.workspaceInit",
        "title": "Open remote git repository"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "GPFS.workspaceInit"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^7.0.43",
    "@types/node-fetch": "^2.5.2",
    "@types/url-parse": "^1.4.3",
    "@types/vscode": "^1.38.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "node-fetch": "^2.6.0",
    "url-parse": "^1.5.8"
  }
}
